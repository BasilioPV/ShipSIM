      function AddRainflowData
        extends Modelica.Icons.Function;
        input Real ini "Start of the new cicle";
        input Real fin "End of the new cicle";
        input Real Sol[4, :];
        input Integer N_Sol;
        output Real Sol_Out[4, :];
        output Integer N_Sol_Out;
      algorithm
        Sol_Out := Sol;
        Sol_Out[1, N_Sol] := ini;
        Sol_Out[2, N_Sol] := fin;
        Sol_Out[3, N_Sol] := abs(fin - ini);
